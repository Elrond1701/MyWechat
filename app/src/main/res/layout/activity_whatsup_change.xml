<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.me.myprofile.change.WhatsUpChangeActivity">

    <TextView
        android:id="@+id/WhatsUpChangeActivity_TextView"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="@string/WhatsUpChangeActivity_Title"
        android:textSize="25sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/WhatsUpChangeActivity_EditText"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textSize="25sp"
        app:layout_constraintTop_toBottomOf="@id/WhatsUpChangeActivity_TextView"
        android:layout_marginTop="50dp"/>

    <Button
        android:id="@+id/WhatsUpChangeActivity_Done"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="@string/WhatsUpChangeActivity_Done"
        android:textSize="25sp"
        app:layout_constraintTop_toBottomOf="@id/WhatsUpChangeActivity_EditText"
        android:layout_marginTop="150dp" />

</androidx.constraintlayout.widget.ConstraintLayout>