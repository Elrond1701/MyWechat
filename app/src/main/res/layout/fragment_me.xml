<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.me.MeFragment">

    <FrameLayout
        android:id="@+id/MeFragment_MyProfile"
        android:name="com.example.mywechat.ui.me.myprofile.MyprofileFragment"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/MeFragment_Line1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintTop_toBottomOf="@id/MeFragment_MyProfile"
        android:layout_marginTop="10dp"
        android:background="#dfdfdf"/>

    <Button
        android:id="@+id/MeFragment_LogOut"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/MeFragment_Line1"
        android:layout_marginTop="50dp"
        android:text="@string/MeFragment_LogOut"/>
</androidx.constraintlayout.widget.ConstraintLayout>